<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#CECDD2"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="415dp"
        android:layout_height="721dp"
        app:srcCompat="@drawable/rectangle_7"
        tools:layout_editor_absoluteX="-2dp"
        tools:layout_editor_absoluteY="222dp" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/border"
        android:backgroundTintMode="multiply"
        app:srcCompat="@drawable/group_287"
        tools:layout_editor_absoluteX="162dp"
        tools:layout_editor_absoluteY="821dp" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/border"
        app:srcCompat="@drawable/group_288"
        tools:layout_editor_absoluteX="217dp"
        tools:layout_editor_absoluteY="821dp" />

    <EditText
        android:id="@+id/editTextTextEmailAddress2"
        android:layout_width="270dp"
        android:layout_height="56dp"
        android:background="@drawable/border"
        android:backgroundTint="#FFFFFF"
        android:drawableLeft="@drawable/person_24dp_e8eaed_fill0_wght400_grad0_opsz24"
        android:drawableTint="#78787A"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:text="   E-mail"
        android:textColor="#D4D3D8"
        tools:layout_editor_absoluteX="70dp"
        tools:layout_editor_absoluteY="446dp" />

    <EditText
        android:id="@+id/editTextTextEmailAddress3"
        android:layout_width="270dp"
        android:layout_height="56dp"
        android:background="@drawable/border"
        android:backgroundTint="#FFFFFF"
        android:drawableLeft="@drawable/person_24dp_e8eaed_fill0_wght400_grad0_opsz24"
        android:drawableTint="#78787A"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:text="   Nome"
        android:textColor="#D4D3D8"
        tools:layout_editor_absoluteX="70dp"
        tools:layout_editor_absoluteY="349dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="172dp"
        android:layout_height="54dp"
        android:backgroundTint="#FFFFFF"
        android:text="Cadastrar"
        android:textColor="#0F044C"
        android:textSize="20dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="119dp"
        tools:layout_editor_absoluteY="666dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ou cadastre-se com"
        android:textColor="#BFBEC4"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="141dp"
        tools:layout_editor_absoluteY="783dp" />

    <EditText
        android:id="@+id/editTextTextPassword2"
        android:layout_width="270dp"
        android:layout_height="56dp"
        android:background="@drawable/border"
        android:backgroundTint="@color/white"
        android:backgroundTintMode="add"
        android:drawableLeft="@drawable/key_24dp_e8eaed_fill0_wght400_grad0_opsz24"
        android:drawableRight="@drawable/visibility_off_24dp_e8eaed_fill0_wght400_grad0_opsz24"
        android:drawableTint="#6C6C6E"
        android:ems="10"
        android:phoneNumber="false"
        android:text="   Senha"
        android:textAllCaps="false"
        android:textColor="#CECDD2"
        android:textColorHighlight="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textColorLink="#FFFEFE"
        android:textCursorDrawable="@drawable/ic_launcher_foreground"
        app:circularflow_angles="100"
        app:circularflow_defaultAngle="100"
        app:circularflow_radiusInDP="100"
        app:circularflow_viewCenter="100"
        app:layout_constraintCircleRadius="100dp"
        tools:layout_editor_absoluteX="70dp"
        tools:layout_editor_absoluteY="546dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/group_282"
        tools:layout_editor_absoluteX="151dp"
        tools:layout_editor_absoluteY="96dp" />

</androidx.constraintlayout.widget.ConstraintLayout>